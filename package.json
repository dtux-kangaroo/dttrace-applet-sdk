{
  "name": "dttrace-applet-sdk",
  "version": "1.0.0",
  "description": "dttrace sdk for applet",
  "main": "index.js",
  "repository": "https://github.com/dtux-kangaroo/dttrace-applet-sdk.git",
  "author": "长卿 <changqing@dtstack.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build:unmin": "npm run clean && cross-env NODE_ENV=development rollup -c -o dist/dttrace-applet.js",
    "build:min": "npm run clean && cross-env NODE_ENV=production rollup -c -o dist/dttrace-applet.min.js",
    "build": "npm run clean && npm run build:commonjs && npm run build::commonjs:min",
    "dev:native": "cross-env NODE_ENV=development rollup -c -o test/native/utils/dttrace-applet.js --watch",
    "dev:mpvue": "cross-env NODE_ENV=development rollup -c -o test/mpvue/src/utils/dttrace-applet.js --watch",
    "dev:wepy": "cross-env NODE_ENV=development rollup -c -o test/wepy/src/utils/dttrace-applet.js --watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-uglify": "^4.0.0"
  }
}
